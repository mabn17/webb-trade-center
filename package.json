  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-utils": "^1.4.1",
    "mocha": "^6.2.0",
    "selenium-webdriver": "^3.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/Components/Items/item.js",
      "!src/serviceWorker.js"
    ]
  }
